{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 60,
      "memory": 1536
    }
  },
  "routes": [
    {
      "src": "^/static/css/(.*)$",
      "dest": "/public/css/$1"
    },
    {
      "src": "^/static/js/(.*)$",
      "dest": "/public/js/$1"
    },
    {
      "src": "^/api/finishline/.*",
      "dest": "/api/main.py"
    },
    {
      "src": "^/api/health$",
      "dest": "/api/main.py"
    },
    {
      "src": "^(?!/api/).*",
      "dest": "/api/http.py"
    }
  ]
}
