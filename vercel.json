{
  "version": 2,
  "builds": [
    { "src": "apps/api/api_main.py", "use": "@vercel/python@3.1.9" },
    { "src": "apps/web/**", "use": "@vercel/static@1.0.0" }
  ],
  "routes": [
    { "src": "^/api/finishline/.*", "dest": "/apps/api/api_main.py" },
    { "src": "^/$", "dest": "/apps/web/index.html" },
    { "src": "^/index.html$", "dest": "/apps/web/index.html" },
    { "src": "/(.*)", "dest": "/apps/web/$1" }
  ]
}