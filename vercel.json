{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 60,
      "memory": 1536
    }
  },
  "routes": [
    {
      "src": "^/(favicon\\.ico|robots\\.txt)$",
      "dest": "/$1"
    },
    {
      "src": "^/(assets|css|js|img|fonts)/(.*)$",
      "dest": "/static/$1/$2"
    },
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^/api/finishline/.*",
      "dest": "/api/main.py"
    },
    {
      "src": "^/api/health$",
      "dest": "/api/main.py"
    },
    {
      "src": "^(?!/api/).*",
      "dest": "/api/http.py"
    }
  ]
}
